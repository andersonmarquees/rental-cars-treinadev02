<%= form_with model: @car, local: true do |f| %>
  <%= f.label :license_plate, 'Placa' %>
  <%= f.text_field :license_plate %>
  <%= f.label :mileage, 'Quilometragem' %>
  <%= f.text_field :mileage %>
  <%= f.label :color, 'Cor' %>
  <%= f.text_field :color %>
  <%= f.label :car_model_id, 'Modelo' %>
  <%= f.collection_select :car_model_id, @car_models, :id, :name  %>
  <%= f.label :subsidiary_id, 'Filial' %>
  <%= f.collection_select :subsidiary_id, @subsidiaries, :id, :name  %>
  <%= f.submit 'Enviar' %>
<% end %>
