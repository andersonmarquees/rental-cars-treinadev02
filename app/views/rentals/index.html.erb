<%= link_to 'Agendar locação', new_rental_path %>

<%= form_with url: search_rentals_path, local: true, method: :get do %>
  <%= label_tag :q, 'Código' %>
  <%= text_field_tag :q %>
  <%= submit_tag 'Buscar' %>
<% end %>

<%#= render partial: 'rental', collection: @rentals %>
<% if @rentals.present? %>
  <%= render @rentals %>
<% end %>
